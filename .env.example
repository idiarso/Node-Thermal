# Application Settings
PORT=3001
NODE_ENV=production

# Printer Configuration
PRINTER_TYPE=EPSON
PRINTER_INTERFACE=printer:EPSON TM-T82X  # Sesuaikan dengan nama printer di Windows
PRINTER_TIMEOUT=5000

# Camera Configuration (CCTV Dahua)
CAMERA_RTSP_URL=rtsp://admin:password@192.168.1.108:554/cam/realmonitor?channel=1&subtype=0
CAMERA_USERNAME=admin
CAMERA_PASSWORD=password

# Arduino Configuration
ARDUINO_PORT=COM3  # Sesuaikan dengan port di Device Manager
ARDUINO_BAUDRATE=9600

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=parking
DB_USER=postgres
DB_PASSWORD=your_password

# Logging Configuration
LOG_LEVEL=info
LOG_ERROR_FILE=logs/error.log
LOG_COMBINED_FILE=logs/combined.log

# Image Capture Settings
CAPTURE_DIR=captures
CAPTURE_FORMAT=jpg
CAPTURE_QUALITY=90

# Security Settings
 